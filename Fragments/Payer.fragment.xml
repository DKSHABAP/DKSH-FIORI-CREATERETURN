<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<SelectDialog title="{i18n>Payer}" liveChange="onLiveChangePayer" multiSelect="false" confirm="onConfirmChangePayer" cancel="onCancelShipTo"
		items="{payerModel>/results}" class="sapUiSizeCompact">
		<StandardListItem title="{parts:[{path:'payerModel>PayerCode'},{path:'payerModel>PayerName'}],formatter:'.formatter.f4ValueBind'}"
			tooltip="{parts:[{path:'payerModel>PayerCode'},{path:'payerModel>PayerName'}],formatter:'.formatter.f4ValueBind'}" type="Active"/>
	</SelectDialog>
</core:FragmentDefinition>